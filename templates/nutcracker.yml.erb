<%= cluster_name %>:
  listen: <%= bind %>:<%= port %>
  hash: <%= hash_function %>
  hash_tag: "<%= hash_tag %>"
  distribution: <%= distribution %>
  auto_eject_hosts: false
  timeout: <%= time_out %>
  <% if redis == true %>
  redis: true
  <% end %>
  <% if servers.any? %>
  servers:
  <% servers.each do |server| %>
    - <%= server %>:6379:1 <%= server %>
  <% end %>
  <% end %>
